package cocos.languagefamily.invalid;

family BoundAbstractFeature {

  featurediagram Architektur {
    Architektur -> ConnectorCorrect? | Automaten?;
    Automaten -> Automatenpruefung;
  }

  abstract feature Architektur;

  feature Automaten {
    component cocos.languagefamily.resources._components.SC;
    wfrs BasicSCCoCos;
    production AutMain -> Architektur.Verhalten;
    gen Main2Java -> Architektur.Verhalten2Java;
  }

  feature Automatenpruefung {
    component general.montiarcexample.statecharts.sccorrect.SCCorrect;
    wfrs TransitionsCorrect;
  }

  feature ConnectorCorrect {
    component general.montiarcexample.connectorCorrectness.ConnectorCorrectness;
    wfrs ConnectorCorrect -> Architektur.ConnectorRule;
  }
}